{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "block-scoped-var":"error",
        "eqeqeq":"error",
        "no-var":"error",
        "prefer-const":"error",
        "eol-last":"error",
        "prefer-arrow-callback":"off",
        "no-trailing-spaces":"error"
    }
}
